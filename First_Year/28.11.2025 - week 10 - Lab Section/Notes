    print("Something went wrong!")
finally:        #Will run whenever the program ends
    print("Bye-bye!")

# 
#  'except' can have a specific name for handling errors
try:
    print(x / 0)
except ZeroDivisionError as err:    #'ZeroDivisionError' is built-in
    print("You can't do that! You tried:", err)    #-> You can't do that! You tried: division by zero


try:
    if x / 0:    
        raise Exception("Nuh-uh! No zero devesions!")   #Calls our Exception since the code falls under our exception
else:
    print("Ok!")

try:
    x = 'Hello!'   
